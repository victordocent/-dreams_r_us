<div class="container">
<h1>Add a list with a selection of movies using simple form</h1>

  <%= simple_form_for @dream do |f| %>
    <%= f.input :title %>
    <%= f.input :availability %>
    <%= f.input :price %>
    <%= f.input :description %>
    <%= f.input :category %>
    <%= f.button :submit %>
  <% end %>
</div>
<%#= link_to "Go back to homepage", root_path %>
